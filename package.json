{
  "name": "telegram-sms-config",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:wasm": "cd wasm-rs && cargo build && wasm-pack build && npx shx cp pkg/* ../src/wasm",
    "preview": "vite preview",
    "postinstall": "curl -O https://github.com/tree-sitter/tree-sitter-bash/releases/download/v0.23.3/tree-sitter-bash.wasm && npx shx mv tree-sitter-bash.wasm public/ && npx shx cp node_modules/web-tree-sitter/tree-sitter.wasm public"
  },
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "crypto-js": "^4.2.0",
    "curlconverter": "^4.11.0",
    "netlify-cli": "^18.0.3",
    "qrious": "^4.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-qrious": "^2.5.6",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.10.2",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "latest",
    "shx": "^0.3.4",
    "typescript": "latest",
    "vite": "latest",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.4.1"
  }
}
